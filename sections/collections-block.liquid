                <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 200 200" width="200" height="200" fill="none">
                  <g fill="#000" clip-path="url(#a)"><path d="M56.053 116.296c-2.445 0-4.374-1.947-4.374-4.417 0-2.469 2.015-4.43 4.42-4.392 2.68.04 4.39 2.335 4.336 4.503-.059 2.383-1.966 4.306-4.385 4.306h.003Z"/><path d="M56.572 126.171c-7.723 0-14.897-2.187-20.793-6.434-6.02-5.302-8.469-14.698-6.354-24.024 1.88-8.294 6.738-15.71 11.023-22.252a1.396 1.396 0 1 1 2.335 1.533c-4.36 6.655-8.869 13.536-10.636 21.337-1.894 8.347.215 16.678 5.37 21.227 7.432 5.342 17.31 7.104 27.687 4.88 12.361-2.653 23.67-10.586 29.518-20.7a1.397 1.397 0 0 1 2.418 1.396c-6.227 10.779-18.24 19.22-31.35 22.031a43.904 43.904 0 0 1-9.226 1.004l.008.002ZM144.403 126.169c-3.002 0-6.082-.331-9.202-1.004-13.108-2.811-25.119-11.255-31.349-22.031a1.395 1.395 0 0 1 .511-1.907 1.395 1.395 0 0 1 1.907.511c5.845 10.114 17.154 18.047 29.515 20.7 10.391 2.23 20.258.462 27.798-4.966 5.047-4.466 7.156-12.796 5.265-21.14-1.773-7.81-6.279-14.688-10.637-21.34a1.397 1.397 0 0 1 2.335-1.533c4.285 6.541 9.143 13.952 11.024 22.25 2.114 9.33-.336 18.726-6.249 23.942-5.98 4.312-13.184 6.518-20.92 6.518h.002Z"/><path d="M141.041 108.275c-.18-.608-.576-1.149-1.076-1.542-.589-.46-1.31-.726-2.047-.847-.296-.048-.6-.073-.893-.024-.861.142-1.507.896-1.829 1.708a4.375 4.375 0 0 0-.296 1.999c.118 1.393.753 2.746 1.694 3.779.641.702 4.45 4.503 4.45 4.503s3.717-3.707 4.449-4.503c.944-1.03 1.574-2.386 1.695-3.779a4.375 4.375 0 0 0-.296-1.999c-.323-.81-.969-1.563-1.829-1.708a2.768 2.768 0 0 0-.894.024c-.737.121-1.458.387-2.047.847-.5.393-.895.934-1.076 1.542"/></g><defs><clipPath id="a"><path fill="#fff" d="M0 0h200v200H0z"/></clipPath></defs>
                </svg>
              {% endif %}
            </div>
            <div class="collections__item__title">
              <h4>
                <b>{{ block.settings.Collection.title }}</b>
              </h4>
            </div>
          </div>
        {%- endfor -%}
      </div>
  </div>

  <div class="collections__wrap">
    {%- for block in section.blocks limit: 1 -%}
      {% comment %}
        Slider
      {% endcomment %}

      <div class="infinity-loader">
        <svg viewBox="-2000 -1000 4000 2000">
          <path id="inf" d="M354-354A500 500 0 1 1 354 354L-354-354A500 500 0 1 0-354 354z"></path>
          <use xlink:href="#inf" stroke-dasharray="1570 5143" stroke-dashoffset="6713px"></use>
        </svg>
      </div>

      <div class="collections__slider [ js-inview ]">
        {% comment %}
          Item
        {% endcomment %}
        <div class="collections__slider__item collections__slider__item--text">
          <h2>
            <b>{{ block.settings.Collection.title }}</b>
          </h2>
          <p>
            <b>{{ block.settings.copy }}</b>
          </p>
          <p>
            <a href="{{ block.settings.Collection.url }}" class="button">Shop all</a>
          </p>
        </div>

        {%- for product in block.settings.Collection.products -%}
          {% comment %}
            Item
          {% endcomment %}
          <a href="{{ product.url }}" class="collections__slider__item collections__slider__item--product">
            <div
              class="collections__slider__item__quickview [ js-show-quickview ]"
              data-tooltip
              data-id="{{ product.handle }}"
              data-collection="{{ block.settings.Collection.title | upcase }}"
            >
              <svg xmlns="http://www.w3.org/2000/svg" width="19.707" height="19.708">
                <path fill="#FFF" fill-rule="nonzero" d="M7.899 0c4.361 0 7.898 3.531 7.898 7.888 0 1.825-.62 3.505-1.663 4.841l5.573 5.563-1.414 1.416-5.577-5.569a7.87 7.87 0 0 1-4.817 1.637C3.537 15.776 0 12.245 0 7.888 0 3.53 3.537 0 7.899 0Zm0 2A5.893 5.893 0 0 0 2 7.888a5.893 5.893 0 0 0 5.899 5.888 5.893 5.893 0 0 0 5.898-5.888A5.893 5.893 0 0 0 7.899 2Z"></path>
              </svg>
              <div class="tooltip">
                <div class="tooltip__wrap">Quick view</div>
              </div>
            </div>

            <div class="collections__slider__item__image">
              {% if product.featured_media %}
                {{ product.featured_media | image_url: width: 400 | image_url: height: 400 | image_tag }}
              {% else %}
                <img
                  src="{{ 'placeholder.jpeg' | asset_url }}"
                  alt="placeholder image"
                  width="400"
                  height="400"
                  loading="lazy"
                >
              {% endif %}
            </div>
            <div class="collections__slider__item__title">
               
              <h6>
                <b>{{ product.title }}</b>
              </h6>
              
              <h6>
                <b>{{ product.price | money }}</b>
              </h6>
            </div>
            <!-- Start of Judge.me code -->
            <div class="jdgm-widget jdgm-preview-badge" data-id="{{ product.id }}">
              {{ product.metafields.judgeme.badge }}
            </div>
            <!-- End of Judge.me code -->
            <div class="collections__slider__item__description">
              <p>{{ product.metafields.custom.short_description }}</p>
            </div>
            
          </a>
        {%- endfor -%}
      </div>
    {%- endfor -%}
  </div>

  <div class="collections__slider__navigation">
    <div class="collections__slider__navigation__prev [ js-collection-slider-prev ]">
      <svg width="80" height="57" viewBox="0 0 80 57" xmlns="http://www.w3.org/2000/svg">
        <path d="M.217 27.968L28.18 0l7.523 7.524L20.049 23.18H80v10.64H20.049l15.653 15.656L28.179 57 .217 29.032a.76.76 0 010-1.064z" fill="#000" fill-rule="nonzero" />
      </svg>
    </div>
    <div class="collections__slider__navigation__next [ js-collection-slider-next ]">
      <svg width="80" height="57" viewBox="0 0 80 57" xmlns="http://www.w3.org/2000/svg">
        <path d="M79.783 27.968L51.82 0l-7.523 7.524L59.951 23.18H0v10.64h59.951L44.298 49.476 51.821 57l27.962-27.968a.76.76 0 000-1.064z" fill="#000" fill-rule="nonzero" />
      </svg>
    </div>
  </div>

  {% comment %}
    Marquee
  {% endcomment %}
  {% comment %} <div class="collections__marquee">
    <div class="collections__marquee__animation">
      <span>{{ section.settings.marquee }}</span>
      <span>{{ section.settings.marquee }}</span>
      <span>{{ section.settings.marquee }}</span>
    </div>
  </div> {% endcomment %}
  

  <div class="marquee-element">
    <div class="marquee-element__track">
      <span><b>{{ section.settings.marquee }}</b></span>
      <span><b>{{ section.settings.marquee }}</b></span>
      <span aria-hidden="true"><b>{{ section.settings.marquee }}</b></span>
      <span aria-hidden="true"><b>{{ section.settings.marquee }}</b></span>
    </div>
  </div>
</div>



{% schema %}
{
  "name": "Collection",
  "settings": [
    {
      "type": "text",
      "id": "marquee",
      "default": "Through life's most vital moments.",
      "label": "Marquee"
    }
  ],
  "max_blocks": 5,
  "blocks": [
    {
      "name": "Collection Item",
      "type": "collection_item",
      "settings": [
        {
          "id": "copy",
          "type": "text",
          "label": "Copy"
        },
        {
          "id": "Collection",
          "type": "collection",
          "label": "Collection"
        }
      ]
    }
  ],
  "presets": [
    {
      "name": "Collection"
    }
  ]
}
{% endschema %}
